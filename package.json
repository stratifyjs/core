{
  "name": "@stratify/core",
  "version": "0.1.0",
  "description": "Dependency Injection package for the Fastify framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "npm run build && npm run test:unit",
    "test:unit": "c8 --check-coverage node scripts/run-tests.js && tsd",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier -w .",
    "benchmark": "npm run build && node benchmarks/run.mjs"
  },
  "keywords": [],
  "author": "jean.antoine.michelet@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.2",
    "autocannon": "^8.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "tsd": "^0.33.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "fastify": "^5.6.0",
    "fastify-plugin": "^5.0.1"
  },
  "tsd": {
    "directory": "src"
  }
}
