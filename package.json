{
  "name": "@stratify/core",
  "version": "0.2.1",
  "description": "Dependency Injection package for the Fastify framework",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "npm run build && npm run test:unit",
    "test:unit": "c8 --check-coverage node scripts/run-tests.js && tsd",
    "lint": "npm run format:check && eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "benchmark": "npm run build && node benchmarks/run.mjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test"
  },
  "author": "jean.antoine.michelet@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@sinclair/typebox": "^0.34.41",
    "@types/node": "^25.0.3",
    "c8": "^10.1.3",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "tsd": "^0.33.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.0.0"
  },
  "tsd": {
    "directory": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stratifyjs/core.git"
  },
  "bugs": {
    "url": "https://github.com/stratifyjs/core/issues"
  },
  "homepage": "https://github.com/stratifyjs/core#readme",
  "keywords": [
    "stratify",
    "fastify",
    "framework",
    "dependency-injection",
    "ioc",
    "di",
    "typescript",
    "nodejs"
  ]
}
